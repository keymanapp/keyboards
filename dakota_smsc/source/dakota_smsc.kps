      <Languages>
        <Language ID="dak">Dakota</Language>
      </Languages>
      <Examples>
        <Example ID="dak" Keys="\" Text="ŋ" Note="Windows: \ is not available on the Dakota keyboard"/>
        <Example ID="dak" Keys="|" Text="Ŋ" Note="Windows: | is not available on the Dakota keyboard"/>
        <Example ID="dak" Keys="`" Text="◌̇" Note="Windows: The &quot;`&quot; key will add an overdot to any letter."/>
        <Example ID="dak" Keys="=" Text="◌́" Note="Windows: The &quot;=&quot; key will add the accent to any letter."/>
        <Example ID="dak" Keys="◌̇" Text="◌̇" Note="Mobile Devices: The ◌̇ key will add an overdot to any letter."/>
        <Example ID="dak" Keys="◌́" Text="◌́" Note="Mobile Devices: The ◌́ key will add an accent to any letter."/>
      </Examples>
    </Keyboard>
