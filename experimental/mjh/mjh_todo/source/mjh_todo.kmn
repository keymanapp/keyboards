c Synch keystrokes to Kushim keyboard set
store(&KEYBOARDVERSION) '1.01'
store(&COPYRIGHT) 'Copyright © Greg Eck'
store(&NAME) 'MJH Todo'
store(&TARGETS) 'any'
store(&VISUALKEYBOARD) 'mjh_todo.kvks'
store(&LAYOUTFILE) 'mjh_todo.keyman-touch-layout'
