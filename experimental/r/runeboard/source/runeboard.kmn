store(&VERSION) '15.0'
store(&TARGETS) 'any'
store(&COPYRIGHT) '© MMXXV ᚱᚢᚾ᛫ᛗᚫᚾ'
store(&NAME) 'Runes'
store(&MESSAGE) 'The Runeboard is designed for typing the English language in Anglo-Saxon Futhorc runes & Proto-Germanic in Elder Futhark runes. However, as it includes the entire Unicode Runic Block, it can also be used for typing a variety of Germanic languages in runes, including Old Norse. Thanks for installing. To learn how to write today' U+0027 's English in runes, visit: https://runerevival.online & watch Rune Revival at: https://www.youtube.com/@LearnRunes'
store(&KEYBOARDVERSION) '4.0'
store(&BITMAP) 'runeboard.ico'
store(&VISUALKEYBOARD) 'runeboard.kvks'
store(&LAYOUTFILE) 'runeboard.keyman-touch-layout'
