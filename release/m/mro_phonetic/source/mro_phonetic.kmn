    + [K_BKSLASH] > "\"
    + [SHIFT K_LBRKT]   > "{"
    + [SHIFT K_RBRKT]   > "}"
    + [RALT K_LBRKT]   > "["
    + [RALT K_RBRKT]   > "]"

    + [SHIFT K_BKSLASH] > "|"
    + [K_COLON]         > ";"
    + [SHIFT K_COLON]   > ":" 
    + [K_QUOTE]         > "'"
    + [SHIFT K_QUOTE]   > '"'
    + [K_COMMA]         > ","
    + [SHIFT K_COMMA]   > "<"
    + [K_PERIOD]        > "."
    + [SHIFT K_PERIOD]  > ">"
    + [K_SLASH]         > "/"
    + [SHIFT K_SLASH]   > "?"

