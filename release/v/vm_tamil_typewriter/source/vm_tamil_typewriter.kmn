store(&VERSION) "10.0" 
store(&NAME) 'Tamil Typewriter (Visual Media)'
store(&COPYRIGHT) '© Visual Media and SIL International'
store(&VISUALKEYBOARD) 'vm_tamil_typewriter.kvks'
store(&BITMAP) 'vm_tamil_typewriter.ico'
c store(&ETHNOLOGUECODE) 'tam'
c store(&LANGUAGE) 'x0449'
store(&MESSAGE) 'Type Tamil using the Tamil typewriter layout'    
store(&HOTKEY) '[CTRL SHIFT K_T]'
store(&KMW_HELPTEXT) 'Visual Media Tamil Typewriter layout'
store(&TARGETS) 'any'
store(&KEYBOARDVERSION) '1.2'
store(&LAYOUTFILE) 'vm_tamil_typewriter.keyman-touch-layout'

begin Unicode > use(main)

group(main) using keys
   + [SHIFT K_0] > ")" 
