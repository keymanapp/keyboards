    c Keyboard options
    store(option_toneplace)     ''

c "w" keystroke produces "ư"
+ "w" > "ư"
c if "u" was typed before "w", the "uw" changes to "ư"
"u" + "w" > "ư" dk(u)
c "w" keystroke after "ư" changes "ư" to "w"
"ư" + "w" > "w"
c unless there was a "u" there before, as marked by dk(u)
"ư" dk(u) + "w" > "uw"
"ư" dk(u) + any(s) > "ứ"
"ư" dk(u) + any(f) > "ừ"
"ư" dk(u) + any(r) > "ử"
"ư" dk(u) + any(x) > "ữ"
"ư" dk(u) + any(j) > "ự"

c "w" keystroke produces "ư"
+ "W" > "Ư"
c if "u" was typed before "w", the "uw" changes to "ư"
"U" + "W" > "Ư" dk(u)
c "w" keystroke after "ư" changes "ư" to "w"
"Ư" + "W" > "W"
c unless there was a "u" there before, as marked by dk(u)
"Ư" dk(u) + "W" > "UW"
"U" + "w" > "Ư" dk(u)
c "w" keystroke after "ư" changes "ư" to "w"
"Ư" + "w" > "W"
c unless there was a "u" there before, as marked by dk(u)
"Ư" dk(u) + "w" > "Uw"
"Ư" dk(u) + any(s) > "Ứ"
"Ư" dk(u) + any(f) > "Ừ"
"Ư" dk(u) + any(r) > "Ử"
"Ư" dk(u) + any(x) > "Ữ"
"Ư" dk(u) + any(j) > "Ự"
c if "u" was typed before "w", the "uw" changes to "ư"
"u" + "W" > "ư" dk(u)
c "w" keystroke after "ư" changes "ư" to "w"
"ư" + "W" > "w"
c unless there was a "u" there before, as marked by dk(u)
"ư" dk(u) + "W" > "uW"

