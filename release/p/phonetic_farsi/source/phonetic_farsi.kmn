c ------------------ Base Keyboard Layout -----------------------
store(&TARGETS) 'desktop'

    begin Unicode > use(main)

    group(main) using keys
    + [K_A] > outs(alef)
    + [K_B] > outs(be)
    + [K_C] > outs(chah)
    + [K_D] > outs(dal)
    + [K_E] > outs(heh)
    + [K_F] > outs(feh)
    + [K_G] > outs(gaf)
    + [K_H] > outs(heh)
    + [K_I] > outs(ya)
    + [K_J] > outs(jeem)
    + [K_K] > outs(keheh)
    + [K_L] > outs(lam)
    + [K_M] > outs(mim)
    + [K_N] > outs(nun)
    + [K_O] > outs(waw)
    + [K_P] > outs(pe)
    + [K_Q] > outs(qaf)
    + [K_R] > outs(ra)
    + [K_S] > outs(seen)
    + [K_T] > outs(teh)
    + [K_U] > outs(waw)
    + [K_V] > outs(waw)
    + [K_W] > outs(waw)
    + [K_X] > outs(khah)
    + [K_Y] > outs(ya)
    + [K_Z] > outs(za)

    + [SHIFT K_A] > outs(ain)
    + [SHIFT K_B] > outs(kaf)
    + [SHIFT K_D] > outs(dad)
    + [SHIFT K_G] > outs(ghain)
    + [SHIFT K_H] > outs(hah)
    + [SHIFT K_K] > outs(khah)
    + [SHIFT K_N] > outs(swish_kaf)
    + [SHIFT K_S] > outs(sad)
    + [SHIFT K_T] > outs(tah)
    + [SHIFT K_Z] > outs(zah)

    + [SHIFT K_Q] > outs(hamza)
    + [SHIFT K_O] > outs(alef_hamza)
    + [SHIFT K_P] > outs(alef_hamza_bottom)

    + [SHIFT K_W] > outs(alef_wasla)
    + [SHIFT K_M] > outs(alef_maksura)
    + [SHIFT K_V] > outs(alef_madda)

    + [SHIFT K_Y] > outs(ya_arab)
    + [SHIFT K_J] > outs(teh_marbuta)
    + [SHIFT K_C] > outs(thousands_sep)
    + [SHIFT K_X] > outs(decimal_sep)
    + [SHIFT K_E] > outs(kashida)

    + [SHIFT K_SPACE]       > outs(non-joiner)
    + [SHIFT K_5]           > outs(percent)
    + [SHIFT K_COMMA]       > outs(guillemet_left)
    + [SHIFT K_PERIOD]      > outs(guillemet_right)
    + [SHIFT K_SLASH]       > outs(arabic_question)
    + [K_COLON]             > outs(arabic_semicolon)
    + [K_COMMA]             > outs(arabic_comma)

    + [SHIFT CTRL K_BKQUOTE] > outs(rtl_marK)

    + [K_0] > outs(zero)
    + [K_9] > outs(nine)
    + [K_8] > outs(eight)
    + [K_7] > outs(seven)
    + [K_6] > outs(six)
    + [K_5] > outs(five)
    + [K_4] > outs(four)
    + [K_3] > outs(three)
    + [K_2] > outs(two)
    + [K_1] > outs(one)


    + [SHIFT K_F] > nul
    + [SHIFT K_I] > nul
    + [SHIFT K_L] > nul
    + [SHIFT K_R] > nul
    + [SHIFT K_U] > nul


