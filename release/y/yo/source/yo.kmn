store(&NAME) 'Yorùbá ìrọ̀rùn'
store(&COPYRIGHT) '© W3Techy Enterprise'
store(&KEYBOARDVERSION) '1.0.2'
store(&TARGETS) 'any'
store(&BITMAP) 'yo.ico'
store(&VISUALKEYBOARD) 'yo.kvks'
store(&LAYOUTFILE) 'yo.keyman-touch-layout'

