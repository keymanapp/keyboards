+ [K_O] > "xʷ"
+ [K_R] > "ʷ"
+ [K_V] > "š"
+ [K_Z] > "x̣"
"u" + "i" > "ƛ̕" c "ui" is glottalized barred lambda

c other
c from Montler's KlallamU layout
c see also: the "rules" section of this *.kmn has additional combinations
"." + "." > "…" c ellipsis

c deadkey - original keys by typing the letter twice
any(desktopGraveKey) + "]" > "]"
any(desktopAcuteKey) + ";" > ";"
