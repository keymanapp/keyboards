store(&NAME) 'X̱aayda-X̱aad Kil'
store(&BITMAP) 'fv_hlgaagilda_xaayda_kil.bmp'
store(&VISUALKEYBOARD) 'fv_hlgaagilda_xaayda_kil.kvk'
begin Unicode > use(main)


group(main) using keys   

c original punctuation
+ [RALT K_PERIOD] > '>'
+ [RALT K_COMMA] > '<'
+ [SHIFT RALT K_BKQUOTE] > '~'
+ [RALT K_BKQUOTE] > '`'
+ [SHIFT RALT K_COLON] > ':'
+ [RALT K_COLON] > ';'
+ [RALT K_SLASH] > '/'
+ [RALT K_QUOTE] > U+0027
+ [RALT K_BKSLASH] > '\'
+ [SHIFT RALT K_BKSLASH] > '|'
+ [SHIFT RALT K_RBRKT] > '}'
+ [SHIFT RALT K_LBRKT] > '{'
+ [RALT K_RBRKT] > ']'
+ [RALT K_LBRKT] > '['
+ [RALT K_EQUAL] > '+'
+ [RALT K_HYPHEN] > '_' 

c Guillemets
+ [SHIFT RALT K_0] > '›'
+ [SHIFT RALT K_9] > '‹'
+ [RALT K_0] > '»'
+ [RALT K_9] > '«'

c Quotes
+ [SHIFT K_RBRKT] > "”"
+ [SHIFT K_LBRKT] > "“"
+ [shift K_bkslash] > "‘"
+ [K_QUOTE] > "’"

+ "`" > "́"
+ "~" > "̀" 
+ "^" > "̂"
+ ";"  > "̱" 
+ "/" > "7"
