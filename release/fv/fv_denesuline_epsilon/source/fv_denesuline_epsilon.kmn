+ [RALT K_RBRKT] > ']'
+ [RALT K_LBRKT] > '['
+ [RALT K_EQUAL] > '='    
+ [RALT shift K_EQUAL] > '+'
+ [RALT K_HYPHEN] > '_' 
+ "<" > ";"
+ ">" > ":"

c Guillemets
+ [SHIFT RALT K_0] > '›'
+ [SHIFT RALT K_9] > '‹'
+ [RALT K_0] > '»'
+ [RALT K_9] > '«'

c Quotes
+ [SHIFT K_RBRKT] > "”"
+ [SHIFT K_equal] > "“"
+ [shift K_BKQUOTE] > "‘"
+ [K_QUOTE] > "’"      

c language

+ [caps shift k_colon] > 'ł'   
+ [ncaps shift k_colon] > 'Ł'
+ [caps k_colon] > 'Ł'
+ [ncaps k_colon] > 'ł'
                                             
+ [caps shift K_LBRKT] > 'ɛ'   
