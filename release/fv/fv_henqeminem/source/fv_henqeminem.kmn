c *************
c CONSTRAINTS
c *************

store(accent)  U+0300 U+0301 U+030C
store(accentkey) "`;~"          

any(accent) + any(accentkey) > context beep

store(toneaccent) U+0300 U+0301 c combining grave, combining acute
store(eje)   U+0313
store(ejekey) "'"



store(vowels_acute) 'áéíóúÁÉÍÓÚ'
store(vowels_grave) 'àèìòùÀÈÌÒÙ'

any(eje) + any(ejekey) > context beep
any(eje) + any(accentkey) > context beep
any(toneaccent) + any(ejekey) > context beep

'ᶿ' + any(ejekey) > context beep                                
'ᶿ' + any(accentkey) > context beep
                  
store(NumbersAndPunctuation) '1234567890-=[]\;,./`~!@#$%^&*()_+{}|:"<>?' "'"  c preventing accents on non-letters
any(NumbersAndPunctuation) + any(accentkey) > context beep                   
any(NumbersAndPunctuation) + any(ejekey) > context beep

c rules for backspacing letters with hacek (to replace with plain letter)
store(withhacek) 'čČšŠ'
store(nohacek) 'cCsS'
store(backspace) [K_BKSP]

any(withhacek) + any(backspace) > index(nohacek,1)

c rules for backspacing vowels with accent
store(plainvowels) "aeiouAEIOU"
store(acutevowels) "áéíóúÁÉÍÓÚ"
any(acutevowels) + [K_BKSP] > index(plainvowels,1)

                  
c ******
c Main
c ******

+ "`" > "̀"   c cdmgra
+ "~" > "̌"   c cdmcar
+ ";" > "́"    c cdmacu               
+ "'" > "̓"    c cdmeje

C single codepoints for vowels with accents and c, s with hacek

store(vowels) 'aeiouAEIOU'
store(acute) ';'
store(grave) '`'
store(hacek) '~'

any(vowels) + any(acute) > index(vowels_acute,1)
any(vowels) + any(grave) > index(vowels_grave,1)
any(nohacek) + any(hacek) > index(withhacek,1)

any(vowels_acute) + any(acute) > context beep
any(vowels_grave) + any(grave) > context beep

